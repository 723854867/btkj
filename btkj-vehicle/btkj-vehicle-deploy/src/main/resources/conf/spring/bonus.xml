<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="bonusManager" class="org.btkj.vehicle.rule.bonus.BonusManager" init-method="_init">
		<property name="vehicleDeptMapper" ref="vehicleDeptMapper" />
		<property name="bonusConfigMapper" ref="bonusConfigMapper" />
		<property name="vehicleBrandMapper" ref="vehicleBrandMapper" />
		<property name="vehicleCoefficientMapper" ref="vehicleCoefficientMapper" />
		<property name="bonusRoutes">
			<map>
				<entry key="profit" value-ref="profit" />
				<entry key="noProfit" value-ref="noProfit" />
				<entry key="ignoreprofit" value-ref="ignoreprofit" />
			</map>
		</property>
	</bean>

	<!-- 营利 -->
	<bean id="profit" class="org.btkj.vehicle.rule.bonus.route.Profit">
		<property name="children">
			<map>
				<entry key="coach" value-ref="profitCoach" />
				<entry key="truck" value-ref="truck" />
			</map>
		</property>
	</bean>

	<!-- 非营利 -->
	<bean id="noProfit" class="org.btkj.vehicle.rule.bonus.route.NoProfit">
		<property name="children">
			<map>
				<entry key="coach" value-ref="noProfitCoach" />
				<entry key="truck" value-ref="truck" />
				<entry key="brand" value-ref="brand" />
				<entry key="transfer" value-ref="transfer" />
			</map>
		</property>
	</bean>

	<!-- 不区分营利 和非营利 -->
	<bean id="ignoreprofit" class="org.btkj.vehicle.rule.bonus.route.IgnoreProfit">
		<property name="children">
			<map>
				<entry key="motor" value-ref="motor" />
				<entry key="tractor" value-ref="tractor" />
				<entry key="paticular" value-ref="particular" />
			</map>
		</property>
	</bean>

	<!-- 车辆种类 -->
	<!-- 营业客车 -->
	<bean id="profitCoach" class="org.btkj.vehicle.rule.bonus.route.vehicle.Coach">
		<property name="children">
			<map>
				<entry key="lease" value-ref="lease" />
				<entry key="cityBus" value-ref="cityBus" />
				<entry key="highwayTransport" value-ref="highwayTransport" />
			</map>
		</property>
	</bean>
	<!-- 非营业客车 -->
	<bean id="noProfitCoach" class="org.btkj.vehicle.rule.bonus.route.vehicle.Coach">
		<property name="children">
			<map>
				<entry key="homeUse" value-ref="homeUse" />
				<entry key="enterprise" value-ref="enterprise" />
				<entry key="organ" value-ref="organ" />
			</map>
		</property>
	</bean>
	
	<!-- 货车 -->
	<bean id="truck" class="org.btkj.vehicle.rule.bonus.route.vehicle.Truck" />
	<!-- 摩托车 -->
	<bean id="motor" class="org.btkj.vehicle.rule.bonus.route.vehicle.Motor" />
	<!-- 拖拉机 -->
	<bean id="tractor" class="org.btkj.vehicle.rule.bonus.route.vehicle.Tractor" />
	<!-- 特种车 -->
	<bean id="particular" class="org.btkj.vehicle.rule.bonus.route.vehicle.Particular" />

	<!-- 品牌 -->
	<bean id="brand" class="org.btkj.vehicle.rule.bonus.route.Brand">
		<property name="vehicleDeptMapper" ref="vehicleDeptMapper" />
		<property name="vehicleBrandMapper" ref="vehicleBrandMapper" />
	</bean>

	<!-- 过户 -->
	<bean id="transfer" class="org.btkj.vehicle.rule.bonus.route.Transfer" />
	
	<!-- 使用性质 -->
	<!-- 家庭自用 -->
	<bean id="homeUse" class="org.btkj.vehicle.rule.bonus.route.CoacheUseType">
		<constructor-arg index="0" value="homeUse" />
		<constructor-arg index="1" value="家庭自用" />
		<property name="children">
			<map>
				<entry key="A012" value-ref="A012" />
				<entry key="A022" value-ref="A022" />
				<entry key="A032" value-ref="A032" />
				<entry key="A042" value-ref="A042" />
				<entry key="A052" value-ref="A052" />
			</map>
		</property>
	</bean>
	<!-- 企业 -->
	<bean id="enterprise" class="org.btkj.vehicle.rule.bonus.route.CoacheUseType">
		<constructor-arg index="0" value="enterprise" />
		<constructor-arg index="1" value="企业" />
		<property name="children">
			<map>
				<entry key="A012" value-ref="A012" />
				<entry key="A022" value-ref="A022" />
				<entry key="A032" value-ref="A032" />
				<entry key="A042" value-ref="A042" />
				<entry key="A052" value-ref="A052" />
			</map>
		</property>
	</bean>
	<!-- 党政事业、机关 -->
	<bean id="organ" class="org.btkj.vehicle.rule.bonus.route.CoacheUseType">
		<constructor-arg index="0" value="organ" />
		<constructor-arg index="1" value="党政事业、机关" />
		<property name="children">
			<map>
				<entry key="A012" value-ref="A012" />
				<entry key="A022" value-ref="A022" />
				<entry key="A032" value-ref="A032" />
				<entry key="A042" value-ref="A042" />
				<entry key="A052" value-ref="A052" />
			</map>
		</property>
	</bean>
	<!-- 出租租匿-->
	<bean id="lease" class="org.btkj.vehicle.rule.bonus.route.CoacheUseType">
		<constructor-arg index="0" value="lease" />
		<constructor-arg index="1" value="出租租匿" />
		<property name="children">
			<map>
				<entry key="A012" value-ref="A012" />
				<entry key="A022" value-ref="A022" />
				<entry key="A032" value-ref="A032" />
				<entry key="A042" value-ref="A042" />
				<entry key="A052" value-ref="A052" />
			</map>
		</property>
	</bean>
	<!-- 城市公交 -->
	<bean id="cityBus" class="org.btkj.vehicle.rule.bonus.route.CoacheUseType">
		<constructor-arg index="0" value="cityBus" />
		<constructor-arg index="1" value="城市公交" />
		<property name="children">
			<map>
				<entry key="A012" value-ref="A012" />
				<entry key="A022" value-ref="A022" />
				<entry key="A032" value-ref="A032" />
				<entry key="A042" value-ref="A042" />
				<entry key="A052" value-ref="A052" />
			</map>
		</property>
	</bean>
	<!-- 公路客运 -->
	<bean id="highwayTransport" class="org.btkj.vehicle.rule.bonus.route.CoacheUseType">
		<constructor-arg index="0" value="highwayTransport" />
		<constructor-arg index="1" value="公路客运 " />
		<property name="children">
			<map>
				<entry key="A012" value-ref="A012" />
				<entry key="A022" value-ref="A022" />
				<entry key="A032" value-ref="A032" />
				<entry key="A042" value-ref="A042" />
				<entry key="A052" value-ref="A052" />
			</map>
		</property>
	</bean>
	
	<!-- 客车种类类型 -->
	<bean id="A012" class="org.btkj.vehicle.rule.bonus.route.CoacheCategory">
		<constructor-arg index="0" value="A012" />
		<constructor-arg index="1" value="6座以下客车 " />
	</bean>
	<bean id="A022" class="org.btkj.vehicle.rule.bonus.route.CoacheCategory">
		<constructor-arg index="0" value="A022" />
		<constructor-arg index="1" value="6及6座以上10座以下客车" />
	</bean>
	<bean id="A032" class="org.btkj.vehicle.rule.bonus.route.CoacheCategory">
		<constructor-arg index="0" value="A032" />
		<constructor-arg index="1" value="10及10座以上20座以下客车 " />
	</bean>
	<bean id="A042" class="org.btkj.vehicle.rule.bonus.route.CoacheCategory">
		<constructor-arg index="0" value="A042" />
		<constructor-arg index="1" value="20及20座以上36座以下客车 " />
	</bean>
	<bean id="A052" class="org.btkj.vehicle.rule.bonus.route.CoacheCategory">
		<constructor-arg index="0" value="A052" />
		<constructor-arg index="1" value="36及366座以上客车" />
	</bean>
</beans>