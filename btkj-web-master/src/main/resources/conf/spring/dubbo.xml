<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
						http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
						
	<dubbo:application name="btkj-master" owner="btkj"/>
	
	<dubbo:registry address="${dubbo.registry.address}" file="${dubbo.register.file}" />
	
	<dubbo:reference id="cloudService" interface="org.btkj.master.api.CloudService" />
	<dubbo:reference id="masterService" interface="org.btkj.master.api.MasterService" />
	
	<dubbo:reference id="appService" interface="org.btkj.user.api.AppService" />
	<dubbo:reference id="userService" interface="org.btkj.user.api.UserService" />
	<dubbo:reference id="tenantService" interface="org.btkj.user.api.TenantService" />
	<dubbo:reference id="employeeService" interface="org.btkj.user.api.EmployeeService" />
	<dubbo:reference id="userManageService" interface="org.btkj.user.api.UserManageService" />
	
	<dubbo:reference id="courierService" interface="org.btkj.courier.api.CourierService" />
	
	<dubbo:reference id="configService" interface="org.btkj.config.api.ConfigService" /> 
	<dubbo:reference id="configManageService" interface="org.btkj.config.api.ConfigManageService" /> 
	
	<dubbo:reference id="nonAutoService" interface="org.btkj.nonauto.api.NonAutoService" />
	
	<dubbo:reference id="biHuManageService" interface="org.btkj.bihu.vehicle.api.BiHuManageService" /> 
	<dubbo:reference id="vehicleManageService" interface="org.btkj.vehicle.api.VehicleManageService" /> 
</beans>